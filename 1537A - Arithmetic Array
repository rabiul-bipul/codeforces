#include <iostream>
using namespace std;

int main()
{
    int t;
    scanf("%d", &t);
    while (t--)
    {
        int n, sum = 0, result;
        scanf("%d", &n);
        int arr[n];
        for (int i = 0; i < n; i++)
        {
            scanf("%d", &arr[i]);
            sum += arr[i];
        }
        if (sum == n)
        {
            printf("%d\n", 0);
        }
        else if (sum < n)
        {
            printf("%d\n", 1);
        }
        else
        {
            result = sum - n;
            printf("%d\n", result);
        }
    }
    return 0;
}
