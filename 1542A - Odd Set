#include <iostream>
using namespace std;

int main()
{
    int t;
    scanf("%d", &t);
    while (t--)
    {
        int n, odd = 0, even = 0, x;
        scanf("%d", &n);
        n *= 2;
        for (int i = 0; i < n; i++)
        {
            scanf("%d", &x);
            if (x % 2 == 0)
                even++;
            else
                odd++;
        }
        if (even == odd)
            printf("Yes\n");
        else
            printf("No\n");
    }
    return 0;
}
