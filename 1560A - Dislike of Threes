#include <stdio.h>
using namespace std;

int main()
{
    int t;
    scanf("%d", &t);

    while (t--)
    {
        int n;
        scanf("%d", &n);
        int count = 1;
        for (int i = 1;; i++)
        {
            if (i % 3 == 0)
            {
                continue;
            }
            if (i % 10 == 3)
            {
                continue;
            }

            if (count == n)
            {
                printf("%d\n", i);
                break;
            }
            count++;
        }
    }
    return 0;
}
